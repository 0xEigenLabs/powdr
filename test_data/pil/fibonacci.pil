constant %N = 16;

namespace std::convert(%N);
    let fe = [];

namespace Fibonacci(%N);
    col fixed ISLAST(i) { std::convert::fe(match i {
        %N - 1 => 1,
        _ => 0,
    })};
    col witness x, y;

    ISLAST * (y' - 1) = 0;
    ISLAST * (x' - 1) = 0;

    (1-ISLAST) * (x' - y) = 0;
    (1-ISLAST) * (y' - (x + y)) = 0;

    public out = y(%N-1);
