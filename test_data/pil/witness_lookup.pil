constant %N = 16;

namespace std::convert(%N);
    let fe = 9;

namespace Quad(%N);
    col fixed id(i) { std::convert::fe(i) };
    col fixed double(i) { std::convert::fe(i * 2) };

    col witness input(i) query ("input", i);
    col witness wdouble;
    col witness quadruple;

    {input, wdouble} in {id, double};
    {wdouble, quadruple} in {id, double};

    public out = quadruple(%N-1);
